{
    "name": "yiisoft/db-oracle",
    "description": "Oracle extension for the Yii framework",
    "keywords": [
        "yii",
        "oracle"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/db-oracle/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/db-oracle"
    },
    "minimum-stability": "dev",
    "require": {
        "ext-pdo": "*",
        "php": "^7.4|^8.0",
        "yiisoft/arrays": "^2.0",
        "yiisoft/db": "^3.0",
        "yiisoft/strings": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.6",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2",
        "yiisoft/active-record": "dev-master",
        "yiisoft/aliases": "^1.1|^2.0",
        "yiisoft/cache": "^1.0",
        "yiisoft/log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\Oracle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Db\\Oracle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
