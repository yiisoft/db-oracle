{
    "name": "yiisoft/db-oracle",
    "description": "Oracle extension for the Yii framework",
    "keywords": [
        "yii",
        "oracle"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/db-oracle/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/db-oracle"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-pdo": "*",
        "ext-pdo_oci": "*",
        "yiisoft/arrays": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/strings": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.17.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.15",
        "yiisoft/aliases": "^1.0",
        "yiisoft/active-record": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Db\\Oracle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Db\\Oracle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
